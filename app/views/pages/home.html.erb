<% @cats.each do |t| %>
        <strong>Name: <%= t.name %></strong><br>    
        <br>  
  <% end %>

<% @users.each do |t| %>
        <strong>Username: <%= t.username %></strong><br>
        <strong>Email: <%= t.email %></strong><br>
        <strong>Age: <%= t.age %></strong><br>
        <br>        
  <% end %>

<% @todos.each do |t| %>
    <strong>Task: <%= t.tasks %></strong><br>
    <strong>Finished: <%= t.finished %></strong><br>
    <br>        
<% end %>

<%= link_to todos_new_path do %>
  <%= content_tag :span, "SUBMIT A NEW TODO" %>
<% end %>